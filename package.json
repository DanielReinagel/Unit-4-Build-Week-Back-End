{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a back-end-web47-water-my-plants",
    "rollbackh": "heroku run knex migrate:rollback -a back-end-web47-water-my-plants",
    "databaseh": "heroku pg:psql -a back-end-web47-water-my-plants",
    "seedh": "heroku run knex seed:run -a back-end-web47-water-my-plants",
    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "16.13.0"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.12",
    "knex-cleaner": "1.3.1",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "cross-env": "7.0.3",
    "eslint": "8.1.0",
    "jest": "27.3.1",
    "nodemon": "2.0.14",
    "supertest": "6.1.6"
  }
}
